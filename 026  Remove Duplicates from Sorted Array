class Solution {
    public int removeDuplicates(int[] nums) {
        int n = 0;  //use a number n to record the number of Duplicate items
        for(int i = 1; i< nums.length; i++){
            if(nums[i] == nums[i-1]){
                n++;
                nums[i-n] = nums[i];
            }else{
                nums[i-n] = nums[i];
            }
        }
        return nums.length-n;
    }
}
