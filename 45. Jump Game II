class Solution {
    public int jump(int[] nums) {
        int cur = 0, i = 0, pre = 0, res = 0, n = nums.length - 1;
        while(cur < n){
            res++;
            pre = cur;
            for(; i <= pre; i++){
                cur = Math.max(cur, nums[i] + i);
            }
            if(cur == pre){
                return -1;
            }
        }
        return res;

    }
}
